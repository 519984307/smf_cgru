<farm>
   <pattern name="Some Host">
      <mask>.*</mask>
      <description>Unrecognized machine</description>
      <capacity>1100</capacity>
      <maxtasks>10</maxtasks>
      <power>1000</power>
<!--
"resources" and "data" for test only (will play role in future)
-->
      <resources>{'c':1000,'m':3000}</resources>
      <data>{'c':'render','m':10}</data>
      <service name="generic" />
      <service name="movgen" />
      <service name="shake" />
      <service name="nuke" />
      <service name="hbatch" />
      <service name="prman" />
      <service name="mantra" />
      <service name="hbatch_prman" />
      <service name="hbatch_mantra" />
      <service name="maya" />
      <service name="blender" />
      <service name="blender_internal" />
      <service name="blender_yafray" />
      <service name="yafray" />
      <service name="xsi" />
      <service name="max" />
<!--
Comment it if you agree with build in os detection (names)
      <os>linux mac windows</os>
You can specify several platforms, if host can take tasks for diffent platforms.
-->
   </pattern>
   <pattern name="Movies">
      <mask>mov.*</mask>
      <description>Movies generation machine</description>
      <os>linux</os>
      <capacity>1100</capacity>
      <maxtasks>2</maxtasks>
      <power>1000</power>
      <service name="generic" />
      <service name="movgen" />
   </pattern>
</farm>
