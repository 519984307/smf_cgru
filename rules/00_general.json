{
"root":".",
"rules":"rules",
"assets":["project","scene","shot"],
"assets_seek":{
	"project":["/"],
	"scene":["/[project]/SCENES/","/[project]/SHOTS/"],
	"shot":["/[project]/SCENES/[scene]/","/[project]/SHOTS/[scene]/"]
},

"assets_data":{
"shot":{
	"source":["SOURCE/SCAN"],
	"result":{
		"path":["RESULT/HRES/DPX","RESULT/DPX","RESULT/HRES/JPG","RESULT/JPG"],
		"cmdexec":"play_sequence"
	},
	"dailies":["RESULT/DAILIES"]
}
},

"OS_unix":{
	"cmdexec":{
		"play_sequence":[
			{"name":"DJV View",
				"cmd":"djv_view @PATH@"},
			{"name":"ls -l",
				"cmd":"xterm -e \"cd @PATH@; ls -l; read -s -n 1\""}
		]
	}
},
"OS_windows":{
	"cmdexec":{
		"play_sequence":[
			{"name":"Pdplayer",
				"cmd":"pdplayer @PATH@"}
		]
	}
},

"company":"CGRU",
"fps":"24",
"dailies":
{
	"resolution":"1280x720",
	"fps":"25"
}
}
